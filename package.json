{
  "name": "@tmclnk/newman-reporter-json-stats",
  "version": "1.0.11",
  "description": "Dumps verbose statistics",
  "main": "index.js",
  "scripts": {
    "pack": "npm pack",
    "local-dev": "npm link && cd node_modules/newman && npm link @tmclnk/newman-reporter-json-stats",
    "test": "node_modules/.bin/newman run examples/cloudwatch-example.postman_collection.json -r @tmclnk/json-stats --verbose"
  },
  "author": "Tom McLaughlin",
  "license": "ISC",
  "peerDependencies": {
    "newman": "^5.2.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "newman": "^5.2.3",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmclnk/newman-reporter-json-stats.git"
  },
  "keywords": [
    "newman",
    "postman",
    "json"
  ],
  "release": {
    "branches": [
      "main",
      "ci"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "bugs": {
    "url": "https://github.com/tmclnk/newman-reporter-json-stats/issues"
  },
  "homepage": "https://github.com/tmclnk/newman-reporter-json-stats#readme",
  "dependencies": {
    "@semantic-release/release-notes-generator": "^10.0.3",
    "chardet": "1.4.0",
    "colors": "^1.4.0",
    "filesize": "8.0.7",
    "liquid-json": "0.3.1",
    "lodash": "4.17.21",
    "postman-collection": "^4.1.5",
    "postman-request": "^2.88.1-postman.31",
    "pretty-ms": "7.0.1",
    "word-wrap": "1.2.3"
  }
}
